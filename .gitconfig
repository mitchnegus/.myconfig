# This is Git's per-user configuration file.
[user]
	name = mitch
[color]
	ui = auto
[alias]
	amend = commit --amend --no-edit
	oust = rm --cached
	unstage = reset HEAD --
	last = log -1 HEAD
	logg = log --graph --pretty=short --all --abbrev-commit --oneline --decorate
	stash-apply-file = "!f() { git diff stash@{0} -- $1 | git apply -R; }; f"
[log]
	date = local
